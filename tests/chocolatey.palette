{
    "eraseAfterPaint": true,
    "paint": [
        {
            "type": "chocolatey",
            "description": "Test to install edison",
            "ensure": "install",
            "software": {
                "edison": "latest"
            }
        },
        {
            "type": "chocolatey",
            "description": "Test to attempt to install edison again",
            "ensure": "install",
            "ignoreFailure": true,
            "software": {
                "edison": "latest"
            }
        }
    ],
    "erase": [
        {
            "type": "chocolatey",
            "description": "Test to uninstall edison",
            "ensure": "uninstall",
            "software": {
                "edison": ""
            }
        }
    ]
}
