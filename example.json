{
    "palette" : {
        "paint": [
            {
            	"type": "software",
            	"names": [ "git", "curl" ],
            	"ensure": "installed",
            	"versions": [ "latest" ]
            },
            {
            	"type": "git",
            	"remote": "https://github.com/Badgerati/Table.git",
            	"localpath": "C:\\Projects\\PicassoTest",
                "localname": "RenamedFolder",
            	"branchname": "master"
            },
            {
                "type": "msbuild",
                "path": "C:\\Windows\\Microsoft.NET\\Framework\\v4.0.30319\\MSBuild.exe",
                "projects": [ "C:\\Projects\\PicassoTest\\RenamedFolder\\C#\\Table.sln" ],
                "arguments": "/p:Configuration=Debug"
            },
            {
                "type": "command",
                "prompt": "powershell",
                "command": "echo Hello, world!"
            },
            {
                "type": "service",
                "name": "Test Service",
                "path": "C:\\full\\absoulte\\path\\to\\service.exe",
                "ensure": "installed",
                "state": "started"
            },
            {
                "type": "copy",
                "from": "C:\\path\\to\\some\\folder",
                "to": "C:\\path\\to\\some\\other\\folder",
                "excludeFiles": [ "*.html", "*.js" ],
                "includeFolders": [ "src" ]
            },
            {
                "type": "vagrant",
                "path": "C:\\path\\to\\project",
                "command": "up"
            },
            {
                "type": "hosts",
                "ensure": "added",
                "ip": "127.0.0.3",
                "hostname": "test.local.com"
            },
            {
                "type": "hosts",
                "ensure": "added",
                "ip": "127.0.0.2",
                "hostname": "test.site.com"
            },
            {
                "type": "website",
                "ensure": "added",
                "state": "started",
                "siteName": "Test Website",
                "appPoolName": "Test Website",
                "hostname": "test.site.com",
                "path": "C:\\Website\\TestWebsite",
                "bindings": [
                    {
                        "ip": "127.0.0.2",
                        "port": "80",
                        "protocol": "http"
                    }
                ]
            }
        ]
    }
}
